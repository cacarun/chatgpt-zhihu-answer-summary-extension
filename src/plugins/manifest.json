{
    "manifest_version": 3,
    "name": "hello-word-plugin",
    "description": "vue3版本的chrome插件",
    "version": "1.0.0",
    "icons": {
        "16": "assets/icon48.png",
        "32": "assets/icon48.png",
        "48": "assets/icon48.png",
        "128": "assets/icon48.png"
    },
    "host_permissions": [
        "https://*.openai.com/"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.zhihu.com/question/*"
            ],
            "js": [
                "js/chunk-vendors.js",
                "js/content.js"
            ],
            "css": [
                "css/content.css"
            ],
            "run_at": "document_end"
        }
    ]
}